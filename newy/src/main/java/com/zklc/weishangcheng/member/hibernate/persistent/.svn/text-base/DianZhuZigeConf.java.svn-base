package com.zklc.weishangcheng.member.hibernate.persistent;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@SuppressWarnings("serial")
@Entity
@Table(name="dianzhu_zige_conf")
public class DianZhuZigeConf implements Serializable{

	private Integer id;						//主键
	private Integer huangjin;				//黄金店资格或者折扣
	private Integer heizuan;				//黑钻店资格或者折扣
	private Integer lanzuan;				//蓝钻店资格或者折扣
	private Integer huangzuan;				//黄钻点资格或者折扣
	private Integer lvzuan;					//绿钻点资格或者折扣
	private Integer hongzuan;				//红钻店资格或者折扣
	private Integer huangguan;				//皇冠店资格或者折扣
	private Integer qijian;					//旗舰店资格或者折扣
	private Integer xihuozong;				//星火总店资格或者折扣
	private Integer xiaofeizong;			//消费总金额资格或者折扣
	private Integer productId;				//产品id不为空的话那么 上面的打折信息有效 id=1 那么资格有效
	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	@Column(name = "id")
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getHuangjin() {
		return huangjin;
	}
	public void setHuangjin(Integer huangjin) {
		this.huangjin = huangjin;
	}
	public Integer getHeizuan() {
		return heizuan;
	}
	public void setHeizuan(Integer heizuan) {
		this.heizuan = heizuan;
	}
	public Integer getLanzuan() {
		return lanzuan;
	}
	public void setLanzuan(Integer lanzuan) {
		this.lanzuan = lanzuan;
	}
	public Integer getHuangzuan() {
		return huangzuan;
	}
	public void setHuangzuan(Integer huangzuan) {
		this.huangzuan = huangzuan;
	}
	public Integer getLvzuan() {
		return lvzuan;
	}
	public void setLvzuan(Integer lvzuan) {
		this.lvzuan = lvzuan;
	}
	public Integer getHongzuan() {
		return hongzuan;
	}
	public void setHongzuan(Integer hongzuan) {
		this.hongzuan = hongzuan;
	}
	public Integer getHuangguan() {
		return huangguan;
	}
	public void setHuangguan(Integer huangguan) {
		this.huangguan = huangguan;
	}
	public Integer getQijian() {
		return qijian;
	}
	public void setQijian(Integer qijian) {
		this.qijian = qijian;
	}
	public Integer getXihuozong() {
		return xihuozong;
	}
	public void setXihuozong(Integer xihuozong) {
		this.xihuozong = xihuozong;
	}
	public Integer getXiaofeizong() {
		return xiaofeizong;
	}
	public void setXiaofeizong(Integer xiaofeizong) {
		this.xiaofeizong = xiaofeizong;
	}
	public Integer getProductId() {
		return productId;
	}
	public void setProductId(Integer productId) {
		this.productId = productId;
	}
	
	
}
